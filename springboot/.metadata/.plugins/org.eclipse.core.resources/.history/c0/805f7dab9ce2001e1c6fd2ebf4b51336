<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper>
<mapper namespace="com.board.mapper.BoardMapper">

  <insert  id="boardInsert">
     INSERT INTO  BOARD (
          BNO,
          TITLE,
          CONTENT,
          WRITER
     ) VALUES (
          seq_bno.nextval,
          #{title},
          #{content},
          #{writer}
     )
  </insert>

</mapper>