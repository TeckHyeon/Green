package com.kdh.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.util.CollectionUtils;
import org.springframework.web.multipart.MultipartHttpServletRequest;

import com.kdh.dto.GametalkPostDto;
import com.kdh.dto.GametalkPostFileDto;
import com.kdh.dto.GametalkUserDto;
import com.kdh.dto.GametalkfollowDto;
import com.kdh.mapper.GametalkMapper;

@Service
public class GametalkServiceImpl implements GametalkService {

	@Autowired
	private GametalkMapper gametalkMapper;
	
	@Override
	public int selectMemberInfoYn(String userId, String userPw) throws Exception {

		return gametalkMapper.selectMemberInfoYn(userId, userPw);
	}
}
