	<insert id="boardInsert" parameterType="board">
		INSERT INTO BOARD (
		BNO,
		TITLE,
		CONTENT,
		WRITER
		) VALUES (
		SEQ_BNO.NEXTVAL,
		#{title},
		#{content},
		#{writer}
		)
	</insert>

</mapper>