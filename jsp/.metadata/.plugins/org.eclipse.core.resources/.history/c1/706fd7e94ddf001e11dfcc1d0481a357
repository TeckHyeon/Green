		<input type="submit" value="로그인1" onclick="submit(A);" />
		<input type="submit" value="로그인2" onclick="submit(B);" />
	</form>
	<script type="text/javascript">
		function submit(e) {
			if (e==A) {
				form.action="A.jsp";
			} else{
				if(e==B) {
					form.action = "B.jsp";
				}
			}
		}
	</script>
